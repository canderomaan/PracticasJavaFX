<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="practica1.controller.EjemploController">
   <children>
      <Label layoutX="88.0" layoutY="57.0" text="Nombre">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="txtNombre" layoutX="81.0" layoutY="94.0" promptText="Escribe Tu Nombre">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label layoutX="84.0" layoutY="143.0" text="Apellidos">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="txtApellidos" layoutX="81.0" layoutY="182.0" promptText="Escribe Tus Apellidos">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="btLimpiar" layoutX="84.0" layoutY="256.0" mnemonicParsing="false" onAction="#btLimpiarClick" text="Limpiar">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btEnviar" layoutX="433.0" layoutY="256.0" mnemonicParsing="false" onAction="#btEnviarClick" text="Enviar">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btSalir" layoutX="78.0" layoutY="319.0" mnemonicParsing="false" onAction="#btSalirClick" prefHeight="32.0" prefWidth="424.0" text="Salir">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
